<div class="body-container">
    <section class="card-container">

        <section class="card">
            <div class="title">
                <h3 id="moduel_title">AD模块</h3>
                <div id="moduel_list">
                    <ul></ul>
                </div>

                <div class="tools">
                    <form id="DataVisual" asp-controller="Device" asp-action="ExportAd">
                        <label>起始时间</label>
                        <input name="begin" id="dateBegin" type="date" />
                        <label>结束时间</label>
                        <input name="stop" id="dateStop" type="date" max="2018-04-10" />
                        <button id="dateButton" type="button" class="btn btn-default">确认</button>
                        <input name="part" type="submit" value="导出" style="width:60px" />
                    </form>

                    <form>
                        <input id="exportAll" asp-controller="Device" asp-action="ExportAdAll" type="submit" value="全部导出" style="width:70px" />
                    </form>

                </div>
            </div>
            <div style="height:545.4px;">
                <!-- 为ECharts准备一个具备大小（宽高）的Dom -->
                <div id="main" style="width: 100%; height:400px;"></div>
            </div>
        </section>
    </section>
    <div class="bottom_tool">
        <button class="btn_soft" id="back" href="/Device/Index">返回</button>

    </div>
</div>

<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/lib/echarts.min.js"></script>
<script src="~/js/history.js"></script>